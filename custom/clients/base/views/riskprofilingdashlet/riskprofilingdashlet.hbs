
{{#eachOptions dashletData}}

{{#eq key 'overallrisk'}}
<div class="risk-profile">
<table style="margin-top: 3%;margin-bottom:1%;width:100%">

   <!-- <tr>
        <th><p style="margin-left: 5%;font-family: inherit;font-weight: bold;line-height:18px;text-align: left;">Overall Risk </p></th>
        <th style="width: 23%;">
                
                        <img src="custom/themes/default/images/{{value}}.png"  style="width: 54%;height:30px;margin-left:36%"/>    
        </th>
    </tr>-->
    <tr>
        <th><p style="margin-left: 5%;font-family: inherit;font-weight: bold;line-height:18px;margin-bottom:1%;text-align: left;">DeltaVista Score </p></th>
        <th style="width: 23%;"><p style="text-align:center;width: 63px;float:right;font-size: 15px;font-weight: normal;margin-bottom:1%">{{../../leadScore}} </p></th>
    </tr>
    <tr>
        <th><p style="margin-left: 5%;font-family: inherit;font-weight: bold;line-height:18px;text-align: left;">Traffic Light </p></th>
        <th style="width: 23%;"><p style="text-align:center;width: 63px;float:right;font-size: 15px;font-weight: normal">{{../../trafficLight}}</p></th>
    </tr>
</table>
</div>

{{else}}

<div class="row-fluid record-panel" data-panelname="panel_body" style="width: 95%;margin-left: 2%;border: 1px solid #E6E6E6;margin-bottom: 2%;">
        <div class="row-fluid record-panel-header panel-inactive" tabindex="-1">
            <h4 class="pull-left" style="margin-left: -3.78%;">
                            {{key}}
                            </h4>
                            <div class="pull-right">
                            {{#eq value ''}} 

                            <!--<img src="custom/themes/default/images/white.jpg"  style="width: 40px;height: 20px;margin-left: 0%; margin-top: -5%;margin-bottom: -7%;/>-->

                             {{else}}

                             
                                <img src="custom/themes/default/images/{{value.flag}}.png"  style="width: 40px;height: 20px;margin-left: 0%; margin-top: -5%;margin-bottom: -7%;"/>

                            
                            {{/eq}}

                                <button class="btn btn-invisible record-panel-arrow">
                                    <i class="fa fa-chevron-down" rel="tooltip" data-container="body" data-title="Toggle Visibility" data-original-title="" title=""></i>
                                </button>
                            </div>
                        </div>
                    

                    
                    <div class="row-fluid record-panel-content " style="display: none;">
                    
                        
                    

                        <h4 class="pull-left" style="background-color: white;border: 0px white;margin-top: -1.5%;">
                                <b style="margin-left: -12%;  ">Attributes</b>
                            </h4>
                        
                         <div> 
                         <h4 class="pull-right my-right-class" style="background-color: white;border: 0px white;    margin-top: 1%;">
                            <b>{{key}}</b>
                            </h4>
                            


                            <table width="95%" style="clear: both;">

                            {{#eachOptions value}}
                            
                                {{#eq key 'flag'}}
                                
                                {{else}}
                                 <tr>
                                     <td >
                                        <h5 class="pull-left" ><p style="word-wrap: break-word;width: 20em;margin-left: 1%;">{{str key ../../../../this.moduleName}}</p></h5>
                                    </td>
                                 </tr>
                                 {{/eq}}

                                {{#eachOptions value}}

                                     
                                    {{#eq value 'YES'}}

                                    {{else}}
                                       <tr>
                                        <td ><p style="word-wrap: break-word;width: 20em;margin-left: 1%;">{{key}}</p></td>

                                        <td ><p>{{value}}</p></td>
                                      </tr>
                                    {{/eq}}

                                {{/eachOptions}}
                            
                            

                            {{/eachOptions}}

                            </table>

                         </div>
                        
                    </div>                
</div>

{{/eq}}

{{/eachOptions}}
